    <fields>
        <fullName>Lotus_Note_Id__c</fullName>
        <caseSensitive>false</caseSensitive>
        <description>This will be used to keep lotus note Id in the system and will be used to update calendar in lotus notes</description>
        <externalId>true</externalId>
        <inlineHelpText>This will be used to keep lotus note Id in the system and will be used to update calendar in lotus notes</inlineHelpText>
        <label>Lotus Note Id</label>
        <length>50</length>
        <required>false</required>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Meeting_Type__c</fullName>
        <externalId>false</externalId>
        <label>Meeting Type</label>
        <picklist>
            <picklistValues>
                <fullName>Demo</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Evaluation</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Modified_By_Mule__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Flag to determine if the event was modified by User or by the Integration Program</description>
        <externalId>false</externalId>
        <inlineHelpText>Always Uncheck this flag while you create or modify the event</inlineHelpText>
        <label>Modified By Mule</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Overall_Status__c</fullName>
        <description>Will keep the overall acceptance of invitee from lotus notes</description>
        <externalId>false</externalId>
        <inlineHelpText>Will keep the overall acceptance of invitee from lotus notes</inlineHelpText>
        <label>Overall Status</label>
        <picklist>
            <picklistValues>
                <fullName>Accepted</fullName>
