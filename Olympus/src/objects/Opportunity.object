    <validationRules>
        <fullName>Enter_lost_reason</fullName>
        <active>true</active>
        <description>If the Lost Reason is other then Lost reason description need to be entered</description>
        <errorConditionFormula>ISPICKVAL(HSFE_Sales_Stage__c,&apos;Closed Lost&apos;) &amp;&amp; ISBLANK(   Text(HSFE_Lost_Reason__c))</errorConditionFormula>
        <errorMessage>Lost Reason is required if Opportunity is Closed Lost.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_lost_reason_description</fullName>
        <active>true</active>
        <description>If the Lost Reason is other then Lost reason description is required</description>
        <errorConditionFormula>ISPICKVAL(  HSFE_Lost_Reason__c ,&apos;Other&apos;) &amp;&amp; ISBLANK(    HSFE_Lost_Reason_Description__c )</errorConditionFormula>
        <errorMessage>Please enter the Close Reason Description since you have selected “Other” as the Close Reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Opportunity_if_Closed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(PRIORVALUE( HSFE_Locked__c ),NOT( $Profile.Name=&quot;System Administrator&quot;),NOT( $Profile.Name=&quot;Support Admin&quot;))</errorConditionFormula>
        <errorMessage>Opportunity cannot be edited once it is Closed, please click Cancel</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Make_Run_Rate_Term_required</fullName>
        <active>true</active>
        <description>If purchase type is run rate, the Run Rate Term(Months) field should be mandatory.</description>
        <errorConditionFormula>ISPICKVAL( HSFE_Purchase_Type__c ,&apos;Run Rate&apos;)&amp;&amp; ( ISNULL(HSFE_Run_Rate_Term_Months__c) || (HSFE_Run_Rate_Term_Months__c == 0) || (HSFE_Run_Rate_Term_Months__c &lt; 0))</errorConditionFormula>
        <errorDisplayField>HSFE_Run_Rate_Term_Months__c</errorDisplayField>
        <errorMessage>Run Rate Term(Months) is required when Purchase Type is Run Rate.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Cannot_moved_to_won_manually</fullName>
        <active>true</active>
        <description>Validation rule to restrict user from manually changing the opportunity stage to closed won.
(HSFE_Quote_Accepted__c == False)&amp;&amp;</description>
        <errorConditionFormula>ISPICKVAL( HSFE_Sales_Stage__c ,&apos;Closed Won&apos;) &amp;&amp;   HSFE_Quote_Accepted__c ==False</errorConditionFormula>
        <errorMessage>Opportunity cannot be moved to Closed Won Stage manually.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_required_on_opportunity_Product</fullName>
        <active>true</active>
        <description>Product required on opportunity before moving it to next stage</description>
        <errorConditionFormula>ISBLANK( Primary_Product_c__c ) &amp;&amp;( ISPICKVAL( HSFE_Sales_Stage__c,&apos;In Funnel&apos;) || ISPICKVAL( HSFE_Sales_Stage__c,&apos;Above Funnel&apos;) || ISPICKVAL( HSFE_Sales_Stage__c,&apos;Best Few&apos;) || ISPICKVAL( HSFE_Sales_Stage__c,&apos;Verbally Accepted&apos;)|| ISPICKVAL( HSFE_Sales_Stage__c,&apos;Closed Won&apos;))</errorConditionFormula>
        <errorMessage>Product required on opportunity before moving it to next Stage</errorMessage>
    </validationRules>
